  "scripts": {
    "test": "NODE_ENV=test && npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && jest --passWithNoTests --runInBand --detectOpenHandles",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "node dist/server.js",
    "db:migration:create": "npx sequelize-cli migration:create --name",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "db:recreate": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed:all"
  },
